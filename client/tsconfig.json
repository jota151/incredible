{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "es2020",
    "target": "es6",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "paths": {
      "@art/*": ["./art/*"]
    }
  },
  "include": ["src/**/*"]
}
